<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <data>
        <record id="stock_picking_form_inherit_process_tracker" model="ir.ui.view">
            <field name="name">stock.picking.form.inherit.process_tracker</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.picking.form"/>
            <field>
                <xpath expr="//header" position="inside">
                    <button 
                        name="do_print_picking" 
                        string="Print Delivery Note" 
                        groups="stock.group_stock_user" 
                        type="object"
                    />
                </xpath>
            </field>
        </record>
    </data>
</odoo>